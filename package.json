{
  "name": "ai-sdk-tool",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "changeset": "changeset",
    "ci:release": "turbo build && changeset publish",
    "ci:version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.2",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  }
}